<%= form_for @artists do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :nationality %>
<%= f.text_field :nationality %>
<br>
<%= f.label :dob %>
<%= f.text_field :dob %>
<br>
<%= f.label :period %>
<%= f.text_field :period %>
